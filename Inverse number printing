#include <stdio.h>
#include <conio.h>
int main()
{
    int num, digit = 0 ;
    while(1){
       printf("\nENTER a number:");
       scanf("%d",&num);
       printf("\nInverse=");
       do{
      digit = num % 10;
      printf("%d",digit);
      num /= 10;
       } while(num !=0);
    }//end of while(1)
    getch();
    return 0;
}
